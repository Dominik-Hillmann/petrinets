package model;

public class PetriNetElement {
	protected String id;
	
	protected PetriNetElement(String id) {
		this.id = id;
	}
	
	/**
	 * Gibt die ID des des Elements aus.
	 * @return Die ID.
	 */
	public String getId() {
		return id;
	}
	
	/**
	 * Gibt aus, ob der Knoten diese ID besitzt.
	 * @param compareId
	 * @return
	 */
	public boolean hasId(String compareId) {
		return compareId.equals(id);
	}
}
